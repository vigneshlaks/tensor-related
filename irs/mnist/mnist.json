{                                                                                                                        
    "metadata": {                                                         
    },                                                                            
    "input": [
      {"id": "input", "op": "const", "dim": [1, 784], "init": "zeros"},
      {"id": "w1", "op": "const", "dim": [784, 128], "init": "import", "path": "irs/mnist/weights/w1.bin", "trainable": true},   
      {"id": "z1", "op": "matmul", "args": ["input", "w1"]},                                 
      {"id": "h1", "op": "relu", "args": ["z1"]},                                                               
      {"id": "w2", "op": "const", "dim": [128, 10], "init": "import", "path": "irs/mnist/weights/w2.bin", "trainable": true},   
      {"id": "logits", "op": "matmul", "args": ["h1", "w2"]},                                  
      {"id": "probs", "op": "softmax", "args": ["logits"]},                                                                
      {"id": "loss", "op": "cross_entropy", "args": ["probs"], "dim": [1, 10]}                                             
    ]                                                                                 
}
